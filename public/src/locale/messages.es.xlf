        <target>Opciones</target>
      </trans-unit>
      <trans-unit id="usage.commands" datatype="html">
        <source>Commands</source>
        <target>Comandos</target>
      </trans-unit>

      <!-- Footer -->
      <trans-unit id="footer.madeBy" datatype="html">
        <source>Made by</source>
        <target>Hecho por</target>
      </trans-unit>
      <trans-unit id="footer.privacy" datatype="html">
        <source>Privacy Policy</source>
        <target>Pol&#237;tica de privacidad</target>
      </trans-unit>
