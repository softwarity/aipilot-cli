      <trans-unit id="installation.windowsAlt" datatype="html"><source>Or download directly from GitHub Releases</source><target>Hoặc tải trực tiếp từ GitHub Releases</target></trans-unit>
      <trans-unit id="installation.prerequisites.title" datatype="html"><source>Prerequisites</source><target>Dieu kien tien quyet</target></trans-unit>
      <trans-unit id="installation.prerequisites.desc" datatype="html"><source>Before starting, install at least one AI coding agent on your computer:</source><target>Truoc khi bat dau, cai dat it nhat mot AI coding agent tren may tinh cua ban:</target></trans-unit>
      <trans-unit id="installation.prerequisites.comingSoon" datatype="html"><source>Coming soon</source><target>Sap ra mat</target></trans-unit>
      <trans-unit id="installation.step2.title" datatype="html"><source>Install the Mobile App</source><target>Cài Đặt Ứng Dụng</target></trans-unit>
      <trans-unit id="installation.step2.desc" datatype="html"><source>Get AIPilot from Google Play or App Store</source><target>Tải AIPilot từ Google Play hoặc App Store</target></trans-unit>
      <trans-unit id="installation.free" datatype="html"><source>Free</source><target>Miễn phí</target></trans-unit>
