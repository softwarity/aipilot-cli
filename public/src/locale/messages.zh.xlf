      <trans-unit id="usage.title" datatype="html"><source>CLI Usage</source><target>CLI使用</target></trans-unit>
      <trans-unit id="usage.basic" datatype="html"><source>Basic usage</source><target>基本用法</target></trans-unit>
      <trans-unit id="usage.options" datatype="html"><source>Options</source><target>选项</target></trans-unit>
      <trans-unit id="usage.commands" datatype="html"><source>Commands</source><target>命令</target></trans-unit>
